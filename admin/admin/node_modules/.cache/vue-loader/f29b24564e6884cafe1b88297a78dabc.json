s=\"select\" v-if=\"type!='info'\"  label=\"教师\" prop=\"jiaoshiId\">\r\n                        <el-select v-model=\"ruleForm.jiaoshiId\" :disabled=\"ro.jiaoshiId\" filterable placeholder=\"请选择教师\" @change=\"jiaoshiChange\">\r\n                            <el-option\r\n                                    v-for=\"(item) in jiaoshiOptions\"\r\n                                    v-bind:key=\"item.id\"\r\n                                    :label=\"item.jiaoshiName\"\r\n                                    :value=\"item.id\">\r\n                            </el-option>\r\n                        </el-select>\r\n                    </el-form-item>\r\n                </el-col>\r\n\r\n                <el-col :span=\"12\"  v-if=\"sessionTable !='jiaoshi' \">\r\n                    <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"教师姓名\" prop=\"jiaoshiName\">\r\n                        <el-input v-model=\"jiaoshiForm.jiaoshiName\"\r\n                                  placeholder=\"教师姓名\" clearable readonly></el-input>\r\n                    </el-form-item>\r\n                    <div v-else>\r\n                        <el-form-item class=\"input\" label=\"教师姓名\" prop=\"jiaoshiName\">\r\n                            <el-input v-model=\"ruleForm.jiaoshiName\"\r\n                                      placeholder=\"教师姓名\" readonly></el-input>\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n                <el-col :span=\"24\" v-if=\"sessionTable !='jiaoshi' \">\r\n                    <el-form-item class=\"upload\" v-if=\"type!='info' && !ro.jiaoshiPhoto\" label=\"图片\" prop=\"jiaoshiPhoto\">\r\n                        <img style=\"margin-right:20px;\" v-bind:key=\"index\" v-for=\"(item,index) in (jiaoshiForm.jiaoshiPhoto || '').split(',')\" :src=\"item\" width=\"100\" height=\"100\">\r\n                    </el-form-item>\r\n                    <div v-else>\r\n                        <el-form-item v-if=\"ruleForm.jiaoshiPhoto\" label=\"图片\" prop=\"jiaoshiPhoto\">\r\n                            <img style=\"margin-right:20px;\" v-bind:key=\"index\" v-for=\"(item,index) in (ruleForm.jiaoshiPhoto || '').split(',')\" :src=\"item\" width=\"100\" height=\"100\">\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n                <el-col :span=\"12\"  v-if=\"sessionTable !='jiaoshi' \">\r\n                    <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"联系方式\" prop=\"jiaoshiPhone\">\r\n                        <el-input v-model=\"jiaoshiForm.jiaoshiPhone\"\r\n                                  placeholder=\"联系方式\" clearable readonly></el-input>\r\n                    </el-form-item>\r\n                    <div v-else>\r\n                        <el-form-item class=\"input\" label=\"联系方式\" prop=\"jiaoshiPhone\">\r\n                            <el-input v-model=\"ruleForm.jiaoshiPhone\"\r\n                                      placeholder=\"联系方式\" readonly></el-input>\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n                <el-col :span=\"12\"  v-if=\"sessionTable !='jiaoshi' \">\r\n                    <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"邮箱\" prop=\"jiaoshiEmail\">\r\n                        <el-input v-model=\"jiaoshiForm.jiaoshiEmail\"\r\n                                  placeholder=\"邮箱\" clearable readonly></el-input>\r\n                    </el-form-item>\r\n                    <div v-else>\r\n                        <el-form-item class=\"input\" label=\"邮箱\" prop=\"jiaoshiEmail\">\r\n                            <el-input v-model=\"ruleForm.jiaoshiEmail\"\r\n                                      placeholder=\"邮箱\" readonly></el-input>\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n                <input id=\"updateId\" name=\"id\" type=\"hidden\">\r\n               <el-col :span=\"12\">\r\n                   <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"资料标题\" prop=\"kechengName\">\r\n                       <el-input v-model=\"ruleForm.kechengName\"\r\n                                 placeholder=\"资料标题\" clearable  :readonly=\"ro.kechengName\"></el-input>\r\n                   </el-form-item>\r\n                   <div v-else>\r\n                       <el-form-item class=\"input\" label=\"资料标题\" prop=\"kechengName\">\r\n                           <el-input v-model=\"ruleForm.kechengName\"\r\n                                     placeholder=\"资料标题\" readonly></el-input>\r\n                       </el-form-item>\r\n                   </div>\r\n               </el-col>\r\n                <el-col :span=\"12\">\r\n                    <el-form-item class=\"select\" v-if=\"type!='info'\"  label=\"资料类型\" prop=\"kechengTypes\">\r\n                        <el-select v-model=\"ruleForm.kechengTypes\" :disabled=\"ro.kechengTypes\" placeholder=\"请选择资料类型\">\r\n                            <el-option\r\n                                v-for=\"(item) in kechengTypesOptions\"\r\n                                v-bind:key=\"item.codeIndex\"\r\n                                :label=\"item.indexName\"\r\n                                :value=\"item.codeIndex\">\r\n                            </el-option>\r\n                        </el-select>\r\n                    </el-form-item>\r\n                    <div v-else>\r\n                        <el-form-item class=\"input\" label=\"资料类型\" prop=\"kechengValue\">\r\n                        <el-input v-model=\"ruleForm.kechengValue\"\r\n                            placeholder=\"资料类型\" readonly></el-input>\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n                <el-col :span=\"24\">\r\n                    <el-form-item class=\"upload\" v-if=\"type!='info' && !ro.kechengPhoto\" label=\"资料图片\" prop=\"kechengPhoto\">\r\n                        <file-upload\r\n                            tip=\"点击上传资料图片\"\r\n                            action=\"file/upload\"\r\n                            :limit=\"3\"\r\n                            :multiple=\"true\"\r\n                            :fileUrls=\"ruleForm.kechengPhoto?ruleForm.kechengPhoto:''\"\r\n                            @change=\"kechengPhotoUploadChange\"\r\n                        ></file-upload>\r\n                    </el-form-item>\r\n                    <div v-else>\r\n                        <el-form-item v-if=\"ruleForm.kechengPhoto\" label=\"资料图片\" prop=\"kechengPhoto\">\r\n                            <img style=\"margin-right:20px;\" v-bind:key=\"index\" v-for=\"(item,index) in (ruleForm.kechengPhoto || '').split(',')\" :src=\"item\" width=\"100\" height=\"100\">\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n                <el-col :span=\"24\">\r\n                    <el-form-item class=\"upload\" v-if=\"type!='info' && !ro.kechengVideo\" label=\"资料视频\" prop=\"kechengVideo\">\r\n                        <file-upload\r\n                            tip=\"点击上传资料视频\"\r\n                            action=\"file/upload\"\r\n                            :limit=\"3\"\r\n                            :multiple=\"true\"\r\n                            :fileUrls=\"ruleForm.kechengVideo?ruleForm.kechengVideo:''\"\r\n                            @change=\"kechengVideoUploadChange\"\r\n                        ></file-upload>\r\n                    </el-form-item>\r\n                    <div v-else>\r\n                        <el-form-item v-if=\"ruleForm.kechengVideo\" label=\"资料视频\" prop=\"kechengVideo\">\r\n                            <img style=\"margin-right:20px;\" v-bind:key=\"index\" v-for=\"(item,index) in ruleForm.kechengVideo.split(',')\" :src=\"item\" width=\"100\" height=\"100\">\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n                <el-col :span=\"24\">\r\n                    <el-form-item class=\"upload\" v-if=\"type!='info' && !ro.kechengFile\" label=\"文档下载\" prop=\"kechengFile\">\r\n                        <file-upload\r\n                                tip=\"点击上传文档下载\"\r\n                                action=\"file/upload\"\r\n                                :limit=\"3\"\r\n                                :multiple=\"true\"\r\n                                :fileUrls=\"ruleForm.kechengFile?ruleForm.kechengFile:''\"\r\n                                @change=\"kechengFileUploadChange\"\r\n                        ></file-upload>\r\n                    </el-form-item>\r\n                    <div v-else>\r\n                        <el-form-item v-if=\"ruleForm.kechengFile\" label=\"文档下载\" prop=\"kechengFile\">\r\n                            <img style=\"margin-right:20px;\" v-bind:key=\"index\" v-for=\"(item,index) in (ruleForm.kechengFile || '').split(',')\" :src=\"item\" width=\"100\" height=\"100\">\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n            <input id=\"jiaoshiId\" name=\"jiaoshiId\" type=\"hidden\">\r\n                <el-col :span=\"24\">\r\n                    <el-form-item v-if=\"type!='info'\"  label=\"资料详情\" prop=\"kechengContent\">\r\n                        <editor style=\"min-width: 200px; max-width: 600px;\"\r\n                                v-model=\"ruleForm.kechengContent\"\r\n                                class=\"editor\"\r\n                                action=\"file/upload\">\r\n                        </editor>\r\n                    </el-form-item>\r\n                    <div v-else>\r\n                        <el-form-item v-if=\"ruleForm.kechengContent\" label=\"资料详情\" prop=\"kechengContent\">\r\n                            <span v-html=\"ruleForm.kechengContent\"></span>\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n            </el-row>\r\n            <el-form-item class=\"btn\">\r\n                <el-button v-if=\"type!='info'\" type=\"primary\" class=\"btn-success\" @click=\"onSubmit\">提交</el-button>\r\n                <el-button v-if=\"type!='info'\" class=\"btn-close\" @click=\"back()\">取消</el-button>\r\n                <el-button v-if=\"type=='info'\" class=\"btn-close\" @click=\"back()\">返回</el-button>\r\n                <el-button v-if=\"role==='学生'\" type=\"primary\" class=\"btn-close\" @click=\"jump()\">留言</el-button>\r\n            </el-form-item>\r\n        </el-form>\r\n    </div>\r\n</template>\r\n<script>\r\n    import styleJs from \"../../../utils/style.js\";\r\n    // 数字，邮件，手机，url，身份证校验\r\n    import { isNumber,isIntNumer,isEmail,isPhone, isMobile,isURL,checkIdCard } from \"@/utils/validate\";\r\n    export default {\r\n        data() {\r\n            return {\r\n                addEditForm:null,\r\n                id: '',\r\n                type: '',\r\n                sessionTable : \"\",//登录账户所在表名\r\n                role : \"\",//权限\r\n                userId:\"\",//当前登录人的id\r\n                jiaoshiForm: {},\r\n                ro:{\r\n                    kechengName: false,\r\n                    kechengTypes: false,\r\n                    kechengPhoto: false,\r\n                    kechengVideo: false,\r\n                    kechengFile: false,\r\n                    jiaoshiId: false,\r\n                    kechengContent: false,\r\n                    insertTime: false,\r\n                },\r\n                ruleForm: {\r\n                    kechengName: '',\r\n                    kechengTypes: '',\r\n                    kechengPhoto: '',\r\n                    kechengVideo: '',\r\n                    kechengFile: '',\r\n                    jiaoshiId: '',\r\n                    kechengContent: '',\r\n                    insertTime: '',\r\n                },\r\n                kechengTypesOptions : [],\r\n                jiaoshiOptions : [],\r\n                rules: {\r\n                   kechengName: [\r\n                              { required: true, message: '资料标题不能为空', trigger: 'blur' },\r\n                          ],\r\n                   kechengTypes: [\r\n                              { required: true, message: '资料类型不能为空', trigger: 'blur' },\r\n                              {  pattern: /^[1-9][0-9]*$/,\r\n                                  message: '只允许输入整数',\r\n                                  trigger: 'blur'\r\n                              }\r\n                          ],\r\n                   kechengPhoto: [\r\n                              { required: true, message: '资料图片不能为空', trigger: 'blur' },\r\n                          ],\r\n                   kechengVideo: [\r\n                              { required: true, message: '资料视频不能为空', trigger: 'blur' },\r\n                          ],\r\n                   kechengFile: [\r\n                              { required: true, message: '文档下载不能为空', trigger: 'blur' },\r\n                          ],\r\n                   jiaoshiId: [\r\n                              { required: true, message: '资料教师不能为空', trigger: 'blur' },\r\n                              {  pattern: /^[1-9][0-9]*$/,\r\n                                  message: '只允许输入整数',\r\n                                  trigger: 'blur'\r\n                              }\r\n                          ],\r\n                   kechengContent: [\r\n                              { required: true, message: '资料详情不能为空', trigger: 'blur' },\r\n                          ],\r\n                   insertTime: [\r\n                              { required: true, message: '发布时间不能为空', trigger: 'blur' },\r\n                          ],\r\n                }\r\n            };\r\n        },\r\n        props: [\"parent\"],\r\n        computed: {\r\n        },\r\n        created() {\r\n            //获取当前登录用户的信息\r\n            this.sessionTable = this.$storage.get(\"sessionTable\");\r\n            this.role = this.$storage.get(\"role\");\r\n            this.userId = this.$storage.get(\"userId\");\r\n\r\n            if (this.role != \"管理员\"){\r\n            }\r\n            this.addEditForm = styleJs.addStyle();\r\n            this.addEditStyleChange()\r\n            this.addEditUploadStyleChange()\r\n            //获取下拉框信息\r\n                this.$http({\r\n                    url:`dictionary/page?page=1&limit=100&sort=&order=&dicCode=kecheng_types`,\r\n                    method: \"get\"\r\n                }).then(({ data }) => {\r\n                    if (data && data.code === 0) {\r\n                        this.kechengTypesOptions = data.data.list;\r\n                    }\r\n                });\r\n\r\n         this.$http({\r\n             url: `jiaoshi/page?page=1&limit=100`,\r\n             method: \"get\"\r\n         }).then(({ data }) => {\r\n             if (data && data.code === 0) {\r\n                this.jiaoshiOptions = data.data.list;\r\n            }\r\n         });\r\n\r\n        },\r\n        mounted() {\r\n        },\r\n        methods: {\r\n\r\n            jump(){\r\n                let router = this.$router\r\n                // name = '/'+name\r\n                router.push('/liuyanguanli').catch(err => err) \r\n            },\r\n            // 下载\r\n            download(file){\r\n                window.open(`${file}`)\r\n            },\r\n            // 初始化\r\n            init(id,type) {\r\n                if (id) {\r\n                    this.id = id;\r\n                    this.type = type;\r\n                }\r\n                if(this.type=='info'||this.type=='else'){\r\n                    this.info(id);\r\n                }\r\n                // 获取用户信息\r\n                this.$http({\r\n                    url:`${this.$storage.get(\"sessionTable\")}/session`,\r\n                    method: \"get\"\r\n                }).then(({ data }) => {\r\n                    if (data && data.code === 0) {\r\n                        var json = data.data;\r\n                    } else {\r\n                        this.$message.error(data.msg);\r\n                    }\r\n                });\r\n            },\r\n            jiaoshiChange(id){\r\n                this.$http({\r\n                    url: `jiaoshi/info/`+id,\r\n                    method: \"get\"\r\n                }).then(({ data }) => {\r\n                    if (data && data.code === 0) {\r\n                        this.jiaoshiForm = data.data;\r\n                    }\r\n                });\r\n            },\r\n            // 多级联动参数\r\n            info(id) {\r\n                let _this =this;\r\n                _this.$http({\r\n                    url: `kecheng/info/${id}`,\r\n                    method: 'get'\r\n                }).then(({ data }) => {\r\n                    if (data && data.code === 0) {\r\n                        _this.ruleForm = data.data;\r\n                        _this.jiaoshiChange(data.data.jiaoshiId)\r\n                    } else {\r\n                        _this.$message.error(data.msg);\r\n                    }\r\n                });\r\n            },\r\n            // 提交\r\n            onSubmit() {\r\n                this.$refs[\"ruleForm\"].validate(valid => {\r\n                    if (valid) {\r\n                        this.$http({\r\n                            url:`kecheng/${!this.ruleForm.id ? \"save\" : \"update\"}`,\r\n                            method: \"post\",\r\n                            data: this.ruleForm\r\n                        }).then(({ data }) => {\r\n                            if (data && data.code === 0) {\r\n                                this.$message({\r\n                                    message: \"操作成功\",\r\n                                    type: \"success\",\r\n                                    duration: 1500,\r\n                                    onClose: () => {\r\n                                        this.parent.showFlag = true;\r\n                                        this.parent.addOrUpdateFlag = false;\r\n                                        this.parent.kechengCrossAddOrUpdateFlag = false;\r\n                                        this.parent.search();\r\n                                        this.parent.contentStyleChange();\r\n                                    }\r\n                                });\r\n                            } else {\r\n                                this.$message.error(data.msg);\r\n                            }\r\n                        });\r\n                    }\r\n                });\r\n                \r\n            },\r\n            // 获取uuid\r\n            getUUID () {\r\n                return new Date().getTime();\r\n            },\r\n            // 返回\r\n            back() {\r\n                this.parent.showFlag = true;\r\n                this.parent.addOrUpdateFlag = false;\r\n                this.parent.kechengCrossAddOrUpdateFlag = false;\r\n                this.parent.contentStyleChange();\r\n            },\r\n            //图片\r\n            kechengPhotoUploadChange(fileUrls){\r\n                this.ruleForm.kechengPhoto = fileUrls;\r\n                this.addEditUploadStyleChange()\r\n            },\r\n            kechengVideoUploadChange(fileUrls){\r\n                this.ruleForm.kechengVideo = fileUrls;\r\n                this.addEditUploadStyleChange()\r\n            },\r\n            kechengFileUploadChange(fileUrls){\r\n                this.ruleForm.kechengFile = fileUrls;\r\n                this.addEditUploadStyleChange()\r\n            },\r\n\r\n            addEditStyleChange() {\r\n                this.$nextTick(()=>{\r\n                    // input\r\n                    document.querySelectorAll('.addEdit-block .input .el-input__inner').forEach(el=>{\r\n                        el.style.height = this.addEditForm.inputHeight\r\n                        el.style.color = this.addEditForm.inputFontColor\r\n                        el.style.fontSize = this.addEditForm.inp